        tools:src="@tools:sample/avatars"
        android:paddingTop="20dp"
        android:layout_marginLeft="15dp"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image12"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@id/image07"
        android:layout_toEndOf="@id/image11"
        tools:src="@tools:sample/avatars"
        android:paddingTop="20dp"
        android:layout_marginLeft="15dp"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image13"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@id/image07"
        android:layout_toEndOf="@id/image12"
        tools:src="@tools:sample/avatars"
        android:paddingTop="20dp"
        android:layout_marginLeft="15dp"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image14"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@id/image07"
        android:layout_toEndOf="@id/image13"
        tools:src="@tools:sample/avatars"
        android:paddingTop="20dp"
        android:layout_marginLeft="15dp"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image15"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_below="@id/image07"
        android:layout_toEndOf="@id/image14"
        tools:src="@tools:sample/avatars"
        android:paddingTop="20dp"
        android:layout_marginLeft="15dp"/>

    <TextView
        android:id="@+id/text07"
        tools:text="Verlo Nuevamente"
        android:paddingLeft="10dp"
        android:paddingTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/image04"/>





</RelativeLayout>